{
	"compilerOptions": {
		"module": "ES6",
		"lib": [
			"es2015",
			"es2017",
			"dom"
		],
		"strict": true,
		"target": "es5",
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"outDir": "temp/vscode-dist",
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"node_modules",
		"library",
		"local",
		"temp",
		"build",
		"settings"
	]
}